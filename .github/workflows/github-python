name: GitHub Actions Demo
run-name: "hello from github-python"
on: [push]
jobs:
  Explore-GitHub-Actions:
    runs-on: ubuntu-latest
    steps:
      - name: Check out repository code
        steps:
        - uses: actions/checkout@v3
        - uses: actions/setup-python@v4
        with:
          python-version: '3.10' 
        - run: python main.py
